{
    "manifest_version": 3,
    "name": "Untitled Shopping Assistant",
    "version": "1.0",
    "description": "A chrome extension that helps you shop what you actually need",
    "background": {
        "service_worker": "js/background.js"
    },
    "action": {
      "default_popup": "html/popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["js/content.js"]
        }
      ],
      "web_accessible_resources": [ 
      {
        "matches": ["<all_urls>"],
        "resources": ["js/util.js", "config.json"]
      }
    ],
      "permissions": [
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "tabs",
        "windows",
        "storage"
      ],
      "host_permissions": [
        "*://*/*"
      ]
  }
  